rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {

    // WARNING: These are temporary, insecure rules for development.
    // They allow any logged-in user to read and write all data.
    // Before deploying to production, you MUST deploy the secure,
    // role-based rules. See README.md for instructions.
    match /{document=**} {
      allow read, write: if request.auth != null;
    }
  }
}
